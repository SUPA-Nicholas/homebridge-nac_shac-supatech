{
  "pluginAlias": "homebridge-nac_shac-supatech",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Name",
        "type": "string",
        "required": true,
        "default": "NAC/SHAC"
      },
      "IPAddress": {
        "title": "NAC/SHAC IP Address",
        "type": "string",
        "format": "ipv4",
        "required": true
      },
      "portNumber": {
        "title": "Port Number",
        "type": "integer",
        "required": true,
        "default": 443
      },
      "Username": {
		"title": "NAC/SHAC Admin Username",
		"type": "string",
		"default": "admin",
		"required": true
	  },
	  "Password": {
		"title": "NAC/SHAC Admin Password",
		"type": "string",
		"required": true
	  }
    }
  }
}